{
  "name": "luwc/webman-yii2-orm",
  "description": "Yii2-style ActiveRecord, Validator, and DataProvider ORM for Webman framework",
  "type": "library",
  "keywords": [
    "webman",
    "yii2",
    "activerecord",
    "validator",
    "dataprovider",
    "orm",
    "database"
  ],
  "homepage": "https://github.com/ak47f16200/webman-yii2-orm.git",
  "license": "MIT",
  "authors": [
    {
      "name": "luwc",
      "email": "ak47f16200@126.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "workerman/webman-framework": "^2.1.0",
    "illuminate/database": "^8.0|^9.0|^10.0",
    "illuminate/validation": "^8.0|^9.0|^10.0",
    "illuminate/pagination": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0|^9.0|^10.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Webman\\Yii2Orm\\": "src/"
    },
    "files": [
      "src/helpers.php",
      "src/helpers/yii_compat.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Webman\\Yii2Orm\\Tests\\": "tests/"
    }
  },
  "extra": {
    "webman": {
      "config": "config/plugin.php"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "Webman\\Yii2Orm\\Install::install"
    ],
    "post-update-cmd": [
      "Webman\\Yii2Orm\\Install::install"
    ],
    "pre-uninstall-cmd": [
      "Webman\\Yii2Orm\\Install::uninstall"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}